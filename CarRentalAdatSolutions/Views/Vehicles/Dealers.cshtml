@model IEnumerable<CarRentalAdatSolutions.Models.ApplicationUser>

@{
    ViewBag.Title = "Dealers";
}

<!--== Page Title Area Start ==-->
<section id="page-title-area" class="section-padding overlay">
    <div class="container">
        <div class="row">
            <!-- Page Title Start -->
            <div class="col-lg-12">
                <div class="section-title  text-center">
                    <h2>Our dealers</h2>
                    <span class="title-line"><i class="fa fa-car"></i></span>
                    @*<p>All dealers you cannrent</p>*@
                </div>
            </div>
            <!-- Page Title End -->
        </div>
    </div>
</section>
<!--== Page Title Area End ==-->

<section id="driver-page-wrap" class="section-padding">
    <div class="container">
        <div class="row">

            @foreach (var item in Model)
            {


                <!-- Single Dealer Start-->
                <div class="col-lg-3 col-md-3">
                    <div class="single-driver-member">
                        <img src="~/Images/default.jpg" alt="Raju Ahammad">
                        <div class="driver-mem-info">
                            <div class="driver-mem-sicons">
                                <a href="#"><i class="fa fa-facebook"></i></a>
                                <a href="#"><i class="fa fa-twitter"></i></a>
                                <a href="#"><i class="fa fa-dribbble"></i></a>
                                <a href="#"><i class="fa fa-pinterest"></i></a>
                            </div>
                            <h4>@Html.DisplayFor(modelItem => item.FullName)<span>Limousine driver</span></h4>
                        </div>
                    </div>
                </div>
               
            }
        </div>
        </div>
    </section>

